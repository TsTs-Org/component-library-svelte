{
  "name": "@tsts-org/component-library-svelte",
  "version": "1.0.13",
  "repository": {
    "type": "git",
    "url": "https://github.com/TsTs-Org/component-library-svelte.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "package": "svelte-package",
    "publish:library": "npm run package && npm publish",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "license": "MIT",
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.0",
    "@storybook/addon-a11y": "^9.0.8",
    "@storybook/addon-docs": "^9.0.8",
    "@storybook/addon-svelte-csf": "^5.0.3",
    "@storybook/addon-vitest": "^9.0.8",
    "@storybook/sveltekit": "^9.0.8",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@vitest/browser": "^3.2.3",
    "@vitest/coverage-v8": "^3.2.3",
    "playwright": "^1.53.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "publint": "^0.3.2",
    "sass": "^1.86.0",
    "storybook": "^9.0.8",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "svelte-preprocess": "^6.0.3",
    "typescript": "^5.0.0",
    "vite": "^6.2.5",
    "vitest": "^3.2.3"
  },
  "keywords": [
    "svelte"
  ],
  "dependencies": {
    "@sveltejs/kit": "^2.16.0",
    "chart.js": "^4.4.9",
    "svelte": "^5.0.0"
  }
}
